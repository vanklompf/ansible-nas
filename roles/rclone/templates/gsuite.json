{
    "type": "service_account",
    "project_id": "{{ rclone_sa_project_id }}",
    "private_key_id": "{{ rclone_sa_private_key_id }}",
    "private_key": "{{ rclone_sa_private_key | replace('\n','\\n') }}",
    "client_email": "{{ rclone_sa_client_email }}",
    "client_id": "{{ rclone_sa_client_id }}",
    "auth_uri": "https://accounts.google.com/o/oauth2/auth",
    "token_uri": "https://oauth2.googleapis.com/token",
    "auth_provider_x509_cert_url": "https://www.googleapis.com/oauth2/v1/certs",
    "client_x509_cert_url": "{{ rclone_sa_client_cert }}"
}